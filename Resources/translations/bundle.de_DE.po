#: @AgitValidationBundle/Plugin/ArrayValidator.php:24
#: @AgitValidationBundle/Plugin/MultiSelectionValidator.php:24
msgid "The value must be an array."
msgstr "Der Wert muss eine Liste sein."

#: @AgitValidationBundle/Plugin/ArrayValidator.php:27
#, php-format
msgid "The array is too short, it must have at least %s elements."
msgstr ""

#: @AgitValidationBundle/Plugin/ArrayValidator.php:30
#, php-format
msgid "The array is too long, it must have at most %s elements."
msgstr ""

#: @AgitValidationBundle/Plugin/ArrayValidator.php:37
msgid "The array has invalid keys."
msgstr ""

#: @AgitValidationBundle/Plugin/ArrayValidator.php:41
#, php-format
msgid "The array is missing the mandatory `%s` key."
msgstr ""

#: @AgitValidationBundle/Plugin/BooleanValidator.php:24
msgid "The value must be a boolean."
msgstr "Der Wert muss ein Boolscher Wert sein."

#: @AgitValidationBundle/Plugin/EmailValidator.php:24
msgid "The e-mail address is malformed."
msgstr "Die E-Mail-Adresse ist ungültig."

#: @AgitValidationBundle/Plugin/EmailValidator.php:28
msgid "The e-mail address must not contain capital letters."
msgstr "Die E-Mail-Adresse darf keine Großbuchstaben enthalten."

#: @AgitValidationBundle/Plugin/FloatValidator.php:24
msgid "The value must be a number."
msgstr "Der Wert muss eine Zahl sein."

#: @AgitValidationBundle/Plugin/FloatValidator.php:27
#: @AgitValidationBundle/Plugin/IntegerValidator.php:27
#, php-format
msgid "The value is too low, it must be higher than %s."
msgstr "Der Wert ist zu niedrig, er muss mindestens %s betragen."

#: @AgitValidationBundle/Plugin/FloatValidator.php:30
#: @AgitValidationBundle/Plugin/IntegerValidator.php:30
#, php-format
msgid "The value is too high, it must be lower than %s."
msgstr "Der Wert ist zu hoch, er darf höchstens %s betragen."

#: @AgitValidationBundle/Plugin/GeolocationValidator.php:34
msgid "The location is off-shore."
msgstr "Der Ort befindet sich nicht auf dem Festland."

#: @AgitValidationBundle/Plugin/GeolocationValidator.php:38
#, php-format
msgid "The geographical location is invalid: %s"
msgstr "Die Geoposition ist ungültig: %s"

#: @AgitValidationBundle/Plugin/IntegerValidator.php:24
msgid "The value must be an integer."
msgstr "Der Wert muss eine Ganzzahl sein."

#: @AgitValidationBundle/Plugin/LatitudeValidator.php:24
msgid "The latitude is invalid."
msgstr "Der Breitengrad ist ungültig."

#: @AgitValidationBundle/Plugin/LongitudeValidator.php:24
msgid "The longitude is invalid."
msgstr "Der Längengrad ist ungültig."

#: @AgitValidationBundle/Plugin/MultiSelectionValidator.php:29
#: @AgitValidationBundle/Plugin/SelectionValidator.php:25
#, php-format
msgid "The value must be one of the following: “%s”."
msgstr "Der Wert muss einer der folgenden sein: „%s“"

#: @AgitValidationBundle/Plugin/MultiSelectionValidator.php:30
#: @AgitValidationBundle/Plugin/SelectionValidator.php:27
msgctxt "quotation inside collection"
msgid "”, “"
msgstr "“, „"

#: @AgitValidationBundle/Plugin/NotNullValidator.php:24
msgid "The value must not be NULL."
msgstr "Der Wert darf nicht NULL sein."

#: @AgitValidationBundle/Plugin/NotRegexValidator.php:24
msgid "The value doesn’t match the required pattern."
msgstr "Der Wert entspricht nicht dem geforderten Format."

#: @AgitValidationBundle/Plugin/NullValidator.php:24
msgid "The value must be NULL."
msgstr "Der Wert muss NULL sein."

#: @AgitValidationBundle/Plugin/NumericValidator.php:24
msgid "The value must be numeric."
msgstr "Der Wert muss numerisch sein."

#: @AgitValidationBundle/Plugin/NumericValidator.php:27
msgid "The value must be a positive integer number."
msgstr "Der Wert muss eine positive Ganzzahl sein."

#: @AgitValidationBundle/Plugin/ObjectValidator.php:24
msgid "The value must be an object."
msgstr "Der Wert muss ein Objekt sein."

#: @AgitValidationBundle/Plugin/ObjectValidator.php:31
msgid "The object has an invalid set of properties."
msgstr "Das Objekt hat ungültige Eigenschaften."

#: @AgitValidationBundle/Plugin/ObjectValidator.php:35
#, php-format
msgid "The object is missing the mandatory “%s” property."
msgstr "Dem Objekt fehlt die Eigenschaft „%s“."

#: @AgitValidationBundle/Plugin/PasswordValidator.php:26
#, php-format
msgid "The password must have at least %d characters."
msgstr "Das Passwort muss mindestens %d Zeichen lang sein."

#: @AgitValidationBundle/Plugin/PasswordValidator.php:29
msgid "The password must contain at least one letter and one number."
msgstr "Das Passwort muss mindestens eine Ziffer und mindestens einen Buchstaben enthalten."

#: @AgitValidationBundle/Plugin/PasswordValidator.php:32
#, php-format
msgid "The password must only consist of letters, numbers and the characters %s."
msgstr "Das Passwort darf nur aus Ziffern, Buchstaben und den Zeichen %s bestehen."

#: @AgitValidationBundle/Plugin/PasswordValidator.php:35
msgid "Both passwords must be identical."
msgstr "Beide Passwörter müssen identisch sein."

#: @AgitValidationBundle/Plugin/RegexValidator.php:24
msgid "The value doesn't match the required pattern."
msgstr "Der Wert entspricht nicht dem geforderten Format."

#: @AgitValidationBundle/Plugin/ScalarValidator.php:24
msgid "The value must be scalar."
msgstr "Der Wert muss skalar sein."

#: @AgitValidationBundle/Plugin/StringValidator.php:24
msgid "The value must be a string."
msgstr "Der Wert muss eine Zeichenkette sein."

#: @AgitValidationBundle/Plugin/StringValidator.php:27
#, php-format
msgid "The value is too short, it must have at least %s characters."
msgstr "Der Wert ist zu kurz, er muss mindestens %s Zeichen lang sein."

#: @AgitValidationBundle/Plugin/StringValidator.php:30
#, php-format
msgid "The value is too long, it must have at most %s characters."
msgstr "Der Wert ist zu lang, er darf höchstens %s Zeichen lang sein."

#: @AgitValidationBundle/Plugin/StringValidator.php:41
msgid "The value contains invalid characters."
msgstr "Der Wert enthält ungültige Zeichen."

#: @AgitValidationBundle/Plugin/UrlValidator.php:24
#: @AgitValidationBundle/Plugin/UrlValidator.php:29
msgid "The URL is invalid."
msgstr ""

#: @AgitValidationBundle/Plugin/UrlValidator.php:32
msgid "The URL is malformed."
msgstr ""

#: @AgitValidationBundle/Service/ValidationService.php:45
#, php-format
msgid "Invalid value for %s: %s"
msgstr "Ungültiger Wert für %s: %s"
