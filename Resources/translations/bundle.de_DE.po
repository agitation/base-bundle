#: @AgitValidationBundle/Validator/ArrayValidator.php:19
#: @AgitValidationBundle/Validator/MultiSelectionValidator.php:19
msgid "The value must be an array."
msgstr "Der Wert muss eine Liste sein."

#: @AgitValidationBundle/Validator/ArrayValidator.php:22
#, php-format
msgid "The list is too short, it must have at least %s elements."
msgstr "Die Liste ist zu kurz, sie muss mindestens %s Elemente haben."

#: @AgitValidationBundle/Validator/ArrayValidator.php:25
#, php-format
msgid "The list is too long, it must have at most %s elements."
msgstr "Die Liste ist zu lang, sie darf höchstens %s Elemente haben."

#: @AgitValidationBundle/Validator/BooleanValidator.php:19
msgid "The value must be a boolean."
msgstr "Der Wert muss ein Boolscher Wert sein."

#: @AgitValidationBundle/Validator/EmailValidator.php:19
msgid "The e-mail address is malformed."
msgstr "Die E-Mail-Adresse ist ungültig."

#: @AgitValidationBundle/Validator/EmailValidator.php:23
msgid "The e-mail address must not contain capital letters."
msgstr "Die E-Mail-Adresse darf keine Großbuchstaben enthalten."

#: @AgitValidationBundle/Validator/FloatValidator.php:19
msgid "The value must be a number."
msgstr "Der Wert muss eine Zahl sein."

#: @AgitValidationBundle/Validator/FloatValidator.php:22
#: @AgitValidationBundle/Validator/IntegerValidator.php:22
#, php-format
msgid "The value is too low, it must be higher than %s."
msgstr "Der Wert ist zu niedrig, er muss mindestens %s betragen."

#: @AgitValidationBundle/Validator/FloatValidator.php:25
#: @AgitValidationBundle/Validator/IntegerValidator.php:25
#, php-format
msgid "The value is too high, it must be lower than %s."
msgstr "Der Wert ist zu hoch, er darf höchstens %s betragen."

#: @AgitValidationBundle/Validator/GeolocationValidator.php:34
msgid "The location is off-shore."
msgstr ""

#: @AgitValidationBundle/Validator/GeolocationValidator.php:38
#, php-format
msgid "The geographical location is invalid: %s"
msgstr "Die Geoposition ist ungültig: %s"

#: @AgitValidationBundle/Validator/IntegerValidator.php:19
msgid "The value must be an integer."
msgstr "Der Wert muss eine Ganzzahl sein."

#: @AgitValidationBundle/Validator/LatitudeValidator.php:19
msgid "The latitude is invalid."
msgstr "Der Breitengrad ist ungültig."

#: @AgitValidationBundle/Validator/LongitudeValidator.php:19
msgid "The longitude is invalid."
msgstr "Der Längengrad ist ungültig."

#: @AgitValidationBundle/Validator/MultiSelectionValidator.php:24
#: @AgitValidationBundle/Validator/SelectionValidator.php:20
#, php-format
msgid "The value must be one of the following: “%s”."
msgstr ""

#: @AgitValidationBundle/Validator/MultiSelectionValidator.php:25
#: @AgitValidationBundle/Validator/SelectionValidator.php:22
msgctxt "quotation inside collection"
msgid "”, “"
msgstr ""

#: @AgitValidationBundle/Validator/NotNullValidator.php:19
msgid "The value must not be NULL."
msgstr "Der Wert darf nicht NULL sein."

#: @AgitValidationBundle/Validator/NotRegexValidator.php:19
msgid "The value doesn’t match the required pattern."
msgstr ""

#: @AgitValidationBundle/Validator/NullValidator.php:19
msgid "The value must be NULL."
msgstr "Der Wert muss NULL sein."

#: @AgitValidationBundle/Validator/NumericValidator.php:19
msgid "The value must be numeric."
msgstr "Der Wert muss numerisch sein."

#: @AgitValidationBundle/Validator/NumericValidator.php:22
msgid "The value must be a positive integer number."
msgstr "Der Wert muss eine positive Ganzzahl sein."

#: @AgitValidationBundle/Validator/ObjectValidator.php:19
msgid "The value must be an object."
msgstr "Der Wert muss ein Objekt sein."

#: @AgitValidationBundle/Validator/ObjectValidator.php:26
msgid "The object has an invalid set of properties."
msgstr "Das Objekt hat ungültige Eigenschaften."

#: @AgitValidationBundle/Validator/ObjectValidator.php:30
#, php-format
msgid "The object is missing the mandatory “%s” property."
msgstr ""

#: @AgitValidationBundle/Validator/PasswordValidator.php:21
#, php-format
msgid "The password must have at least %d characters."
msgstr "Das Passwort muss mindestens %d Zeichen lang sein."

#: @AgitValidationBundle/Validator/PasswordValidator.php:24
msgid "The password must contain at least one letter and one number."
msgstr "Das Passwort muss mindestens eine Ziffer und mindestens einen Buchstaben enthalten."

#: @AgitValidationBundle/Validator/PasswordValidator.php:27
#, php-format
msgid "The password must only consist of letters, numbers and the characters %s."
msgstr "Das Passwort darf nur aus Ziffern, Buchstaben und den Zeichen %s bestehen."

#: @AgitValidationBundle/Validator/PasswordValidator.php:30
msgid "Both passwords must be identical."
msgstr "Beide Passwörter müssen identisch sein."

#: @AgitValidationBundle/Validator/RegexValidator.php:19
msgid "The value doesn't match the required pattern."
msgstr "Der Wert entspricht nicht dem geforderten Format."

#: @AgitValidationBundle/Validator/ScalarValidator.php:19
msgid "The value must be scalar."
msgstr "Der Wert muss skalar sein."

#: @AgitValidationBundle/Validator/StringValidator.php:19
msgid "The value must be a string."
msgstr "Der Wert muss eine Zeichenkette sein."

#: @AgitValidationBundle/Validator/StringValidator.php:22
#, php-format
msgid "The value is too short, it must have at least %s characters."
msgstr "Der Wert ist zu kurz, er muss mindestens %s Zeichen lang sein."

#: @AgitValidationBundle/Validator/StringValidator.php:25
#, php-format
msgid "The value is too long, it must have at most %s characters."
msgstr "Der Wert ist zu lang, er darf höchstens %s Zeichen lang sein."

#: @AgitValidationBundle/Validator/StringValidator.php:36
msgid "The value contains invalid characters."
msgstr "Der Wert enthält ungültige Zeichen."

#: @AgitValidationBundle/Validator/UrlValidator.php:19
#: @AgitValidationBundle/Validator/UrlValidator.php:24
msgid "The website is invalid."
msgstr "Die Website ist ungültig."

#: @AgitValidationBundle/Validator/UrlValidator.php:27
msgid "The website is malformed."
msgstr "Die Webadresse ist ungültig."
